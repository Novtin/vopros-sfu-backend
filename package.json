  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --runInBand --config jest.config.js",
    "test:watch": "test --watch",
    "test:cov": "test --coverage",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:create": "nest build & npm run typeorm migration:create src/migrations/Migration",
    "migration:generate": "nest build & npm run typeorm -- -d src/config/orm.datasource.ts migration:generate src/migrations/Migration",
    "migration:run": "nest build & npm run typeorm migration:run -- -d src/config/orm.datasource.ts"
  },
